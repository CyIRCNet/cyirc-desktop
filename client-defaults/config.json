{
	"windowTitle": "CyIRC",
	"startupScreen": "welcome",
	"kiwiServer": "https://localdev.clients.kiwiirc.com/webirc/kiwiirc/",
	"restricted": true,
	"sidebarDefault": true,
	"direct":true,
	"theme": "Dark",
	"remember_buffers":true,
	"state_key":"kiwi",
	"themes": [
		{ "name": "Default", "url": "static/themes/default" },
		{ "name": "Dark", "url": "static/themes/dark" },
		{ "name": "Coffee", "url": "static/themes/coffee" },
  		{ "name": "GrayFox", "url": "static/themes/grayfox" },
		{ "name": "Nightswatch", "url": "static/themes/nightswatch" },
		{ "name": "Osprey", "url": "static/themes/osprey" },
		{ "name": "Radioactive", "url": "static/themes/radioactive" },
		{ "name": "Sky", "url": "static/themes/sky" },
		{ "name": "Elite", "url": "static/themes/elite" }
	],
	"warnOnExit": true,
	"disconnectOnSaslFail": true,
	"showChanlistModes": true,
	"showColorPicker": true,
	"teamHighlights": true,
	"startupOptions" : {
		"greetingText": "Welcome!",
		"server": "irc.cyirc.org",
		"port": 6697,
		"tls": true,
		"direct": true,
		"channel": "#cyprus",
		"nick": "user?",
		"version": "CyIRC 1.0.2",
		"messageLayout": "modern",
		"buttonText": "Start",
		"infoContent": "CyIRC Rocks!",
		"infoBackground": "img/background.jpg"
	},
	"buffers": {
        	"show_hostnames": true,
        	"show_realnames": true,
        	"show_emoticons": true,
        	"traffic_as_activity": true,
		"auto_reconnect": false,
		"show_joinparts": true,
		"extra_formatting": true,
		"quit": "Quit"
	},
	"embedly": {
		"key": ""
	},
	"plugins": [
		{"name": "emoji-js", "url": "plugins/emoji.js"},
		{"name": "replace-emoji", "url": "plugins/replace-emoji.js"},
		{"name": "sysinfo", "url": "plugins/sysinfo.js"},
		{"name": "nativesocks", "url": "plugins/nativesocks.js"},
		{"name": "title", "url":"plugins/title.js"}
	]
}
